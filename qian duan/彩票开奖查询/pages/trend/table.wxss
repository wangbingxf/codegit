.line{
  /* width:100px;
  transform: rotate(270rad); */
  position:absolute;
  left:50%;
  top:50%;
  height:'1px';
  line-height: 1px;
  margin-top: -1px;
  box-sizing: border-box;
  background-color:#CDC1A6;
  transform-origin: 'left center';
  pointer-events: none;
  z-index: 10;
}
