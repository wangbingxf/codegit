.bottom {
    /* position: fixed;

    bottom: calc(constant(safe-area-inset-bottom) + 20px);
    bottom: calc(env(safe-area-inset-bottom) + 20px); */
    margin-top: 40rpx;
    padding-bottom: 40rpx;
    left: 0;
    right: 0;
    z-index:2000;
}

.bottom .button {
    width: 100px;
    height: 40px;
    border-radius: 10px;
    background: #f2ad68;
    color: #fff;
    text-align: center;
    line-height: 40px;
    float: left;
    margin-left: 10px;
    width: calc((100% - 30px) / 2);
    overflow: hidden;
    font-size: 16px;
}

.bottom .button button:not([size=mini]) {
    background: none;
    color: #fff;
    font-weight: normal;
    margin: 0;
    padding: 0;
    line-height: 40px;
    width: 100%;
    font-size: 16px;
}

.bottom .button:nth-of-type(2) {
    background: #ea5a35;
}