page {
    background: #fff;
}

.list {
    border-bottom: solid 1px #eee;
    padding: 10px;
    position: relative;
}

.list .action {
    position: absolute;
    right: 10px;
    top: 50%;
    margin-top: -7.5px;
}

.list .action image {
    height: 15px;
    width: 26px;
}

.list .title {
    color: #9d9d9d;
}

.list .title text {
    margin-right: 10px;
}

.lottery_item_ball {
    margin-top: 10px;
}

.lottery_item_ball_red {
    float: left;
    border-radius: 50%;
    width: 27px;
    height: 30px;
    text-align: center;
    line-height: 28px;
    margin-right: 10px;
    color: #fff;
    font-size: 14px;
    background: url(data:image/png;base64,);
    background-size: cover;
}

.lottery_item_ball_blue {
    float: left;
    border-radius: 50%;
    width: 30px;
    height: 28px;
    text-align: center;
    line-height: 28px;
    margin-right: 10px;
    color: #372048;
    font-size: 14px;
    background: url(data:image/png;base64,);
    background-size: cover;
}

.item .item_text {
    text-align: center;
    line-height: 60px;
    font-size: 16px;
}

.item .item_text text:nth-of-type(2) {
    margin-left: 10px;
}

.table {
    border: solid 2px #dadada;
    margin: 0 10px;
    margin-top: 0;
}

.table .th {
    float: left;
    background: #e0e0e0;
    line-height: 40px;
    text-align: center;
    border-right: solid 2px #dadada;
    width: calc((100% - 4px) / 3);
}

.table .th:nth-of-type(3) {
    border-right: 0;
}

.table .td {
    float: left;
    line-height: 40px;
    border-right: solid 2px #dadada;
    border-top: solid 2px #dadada;
    width: calc((100% - 4px) / 3);
    text-align: center;
    font-size: 16px;
}

.table .td:nth-of-type(3) {
    border-right: 0;
}