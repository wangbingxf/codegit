.banner image {
    width: 100%;
    height: 100%;
}

.message {
    background: #ee8e83;
    color: #fff;
    font-size: 14px;
    line-height: 30px;
}

.message_icon {
    width: 15%;
    float: left;
    margin-left: 10px;
}

.message_text {
    overflow: hidden;
    white-space: nowrap;
    line-height: 30px;
    float: left;
    margin-right: 10px;
    width: calc(100% - 15% - 20px);
}

.lottery_item {
    background: #fff;
    border-radius: 10px;
    padding: 10px;
    margin: 10px;
    margin-bottom: 0;
    position: relative;
}

.history {
    position: absolute;
    right: 10px;
    top: 10px;
    background: #4a82be;
    border-radius: 10px;
    color: #fff;
    line-height: 26px;
    padding: 0 10px;
    font-size: 14px;
}

.subscription {
    background: #4a82be;
    border-radius: 10px;
    color: #fff;
    line-height: 40px;
    padding: 0 10px;
    font-size: 16px;
    margin: 10px;
    text-align: center;
}

.lottery_item_title {
    color: #333;
    line-height: 30px;
    font-size: 16px;
}

.lottery_item_code {
    line-height: 20px;
    color: #9d9d9d;
    font-size: 12px;
}

.lottery_item_ball {
    margin-top: 5px;
}

.lottery_item_ball_red {
    float: left;
    border-radius: 50%;
    width: 27px;
    height: 30px;
    text-align: center;
    line-height: 28px;
    margin-right: 10px;
    color: #fff;
    font-size: 14px;
    background: url(data:image/png;base64,);
    background-size: cover;
}

.lottery_item_ball_blue {
    float: left;
    border-radius: 50%;
    width: 30px;
    height: 28px;
    text-align: center;
    line-height: 28px;
    margin-right: 10px;
    color: #372048;
    font-size: 14px;
    background: url(data:image/png;base64,);
    background-size: cover;
}

.copyright {
    line-height: 40px;
    text-align: center;
    color: #fff;
    font-size: 14px;
}

.adv {
    margin-top: 10px;
}